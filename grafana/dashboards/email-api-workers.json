{
    "title": "Email API Workers",
    "timezone": "browser",
    "schemaVersion": 42,
    "version": 1,
    "templating": {
      "list": [
        {
          "name": "client_plan",
          "type": "query",
          "includeAll": true,
          "allValue": ".*",
          "multi": true,
          "definition": "label_values(email_validation_business_validations_total,client_plan)",
          "query": {
            "qryType": 1,
            "query": "label_values(email_validation_business_validations_total,client_plan)",
            "refId": "PrometheusVariableQueryEditor-VariableQuery"
          },
          "refresh": 1,
          "regex": "",
          "sort": 1,
          "current": { "text": "All", "value": "$__all" }
        },
        {
          "name": "validation_type",
          "type": "query",
          "includeAll": true,
          "allValue": ".*",
          "multi": true,
          "definition": "label_values(email_validation_business_validations_total,validation_type)",
          "query": {
            "qryType": 1,
            "query": "label_values(email_validation_business_validations_total,validation_type)",
            "refId": "PrometheusVariableQueryEditor-VariableQuery"
          },
          "refresh": 1,
          "regex": "",
          "sort": 1,
          "current": { "text": "All", "value": "$__all" }
        }
      ]
    },
    "panels": [
      {
        "type": "stat",
        "title": "Backlog agregado",
        "datasource": { "type": "prometheus", "uid": "prometheus" },
        "targets": [
          { "expr": "max without(instance)(email_validation_business_job_queue_depth)", "legendFormat": "backlog", "refId": "A" }
        ],
        "fieldConfig": { "defaults": { "unit": "none", "decimals": 0 }, "overrides": [] },
        "options": { "reduceOptions": { "calcs": ["lastNotNull"] } },
        "gridPos": { "h": 6, "w": 6, "x": 0, "y": 0 }
      },
      {
        "type": "timeseries",
        "title": "Backlog por instancia",
        "datasource": { "type": "prometheus", "uid": "prometheus" },
        "targets": [
          { "expr": "email_validation_business_job_queue_depth", "legendFormat": "{{instance}}", "refId": "A" }
        ],
        "fieldConfig": { "defaults": { "unit": "none" }, "overrides": [] },
        "gridPos": { "h": 6, "w": 18, "x": 6, "y": 0 }
      },
      {
        "type": "timeseries",
        "title": "Producci贸n vs Consumo (req/s)",
        "datasource": { "type": "prometheus", "uid": "prometheus" },
        "targets": [
          {
            "expr": "sum(rate(email_validation_business_validations_total{client_plan=~\"$client_plan\",validation_type=~\"$validation_type\"}[5m]))",
            "legendFormat": "producci贸n",
            "refId": "A"
          },
          {
            "expr": "sum(rate(email_validation_business_jobs_consumed_total{client_plan=~\"$client_plan\",validation_type=~\"$validation_type\"}[5m]))",
            "legendFormat": "consumo",
            "refId": "B"
          }
        ],
        "fieldConfig": { "defaults": { "unit": "ops" }, "overrides": [] },
        "gridPos": { "h": 8, "w": 24, "x": 0, "y": 6 }
      },
      {
        "type": "timeseries",
        "title": "Job p95 (s) por instancia",
        "datasource": { "type": "prometheus", "uid": "prometheus" },
        "targets": [
          {
            "expr": "histogram_quantile(0.95, sum by (instance, le) (rate(email_validation_business_job_duration_seconds_bucket{client_plan=~\"$client_plan\",validation_type=~\"$validation_type\"}[10m])))",
            "legendFormat": "{{instance}}",
            "refId": "A"
          }
        ],
        "fieldConfig": { "defaults": { "unit": "s" }, "overrides": [] },
        "gridPos": { "h": 8, "w": 12, "x": 0, "y": 14 }
      },
      {
        "type": "timeseries",
        "title": "Errores por instancia (5m)",
        "datasource": { "type": "prometheus", "uid": "prometheus" },
        "targets": [
          {
            "expr": "sum by (instance) (rate(email_validation_business_job_errors_total{client_plan=~\"$client_plan\",validation_type=~\"$validation_type\"}[5m]))",
            "legendFormat": "{{instance}}",
            "refId": "A"
          }
        ],
        "fieldConfig": { "defaults": { "unit": "ops" }, "overrides": [] },
        "gridPos": { "h": 8, "w": 12, "x": 12, "y": 14 }
      },
      {
        "type": "timeseries",
        "title": "Saturaci贸n worker (backlog/consumo)",
        "datasource": { "type": "prometheus", "uid": "prometheus" },
        "targets": [
          {
            "expr": "max without(instance)(email_validation_business_job_queue_depth) / clamp_min(sum(rate(email_validation_business_jobs_consumed_total[5m])), 1e-9)",
            "legendFormat": "saturaci贸n",
            "refId": "A"
          }
        ],
        "fieldConfig": { "defaults": { "unit": "s" }, "overrides": [] },
        "gridPos": { "h": 8, "w": 24, "x": 0, "y": 22 }
      }
    ],
    "time": { "from": "now-1h", "to": "now" }
  }
  